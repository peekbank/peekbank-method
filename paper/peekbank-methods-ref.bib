  keywords = {Eye-tracking,Lexical processing,Looking-while-listening,read,useful,Visual world paradigm,Vocabulary development,Word recognition},
  file = {/home/vboyce/Zotero/storage/XTF8F93U/Zettersten et al. - 2023 - Peekbank An open, large-scale repository for deve.pdf}
}
