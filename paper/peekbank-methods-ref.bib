  file = {/home/vboyce/Zotero/storage/2966M3LA/López Pérez et al. - 2025 - Infants' Knowledge of Individual Words Investigating Links Between Parent Report and Looking Time.pdf;/home/vboyce/Zotero/storage/EMKBG7FN/infa.html}
}

@article{nystrom2025,
